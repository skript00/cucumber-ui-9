{
  "name": "cucumber-ui-9",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:dryRun": "npx cypress-cucumber-diagnostics",
    "test:smoke": "cypress run --env tags=@Smoke",
    "test:regression": "cypress run --env tags=@Regression",
    "test:multiple": "cypress run --env tags=\"@Smoke or @Northwestern\"",
    "test:ignore": "cypress run --env tags=\"@Regression and not @HtmlElements\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "devDependencies": {
    "@badeball/cypress-cucumber-preprocessor": "^20.0.5",
    "@bahmutov/cypress-esbuild-preprocessor": "^2.2.1",
    "@eslint/js": "^9.4.0",
    "cypress": "^13.10.0",
    "eslint": "^9.4.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.3.0",
    "multiple-cucumber-html-reporter": "^3.6.2",
    "prettier": "^3.3.0"
  },
  "cypress-cucumber-preprocessor": {
    "stepDefinitions": "cypress/e2e/step-definitions/**/*.js",
    "html": {
      "enabled": true,
      "output": "reports/cucumber-html/cucumber-report.html"
    },
    "messages": {
      "enabled": true,
      "output": "reports/cucumber-ndjson/cucumber-report.ndjson"
    },
    "json": {
      "enabled": true,
      "output": "reports/cucumber-json/cucumber-report.json"
    }
  }
}
